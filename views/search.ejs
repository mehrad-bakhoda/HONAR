<%-include("head");-%>


<section class="page searchPage">
    <%-include("header");-%>
    <section class="contents">
        <div class="container-fluid">
            <div class="row">
              <% searched.forEach(function(product) { %>
                <div class="col-xl-3 col-lg-4 col-sm-6 col-xs-12 result">
                    <a href="/Product/<%=product.productId%>/<%=product.fileName%>">
                        <div class="card product">
                            <div class="card-body">
                                <img src="/<%=product.coverPath%>" />
                            </div>
                            <div class="card-footer">
                                <p> <%=product.fileName%> </p>
                                <p> <%=product.user.firstName%> </p>
                            </div>
                        </div>
                    </a>
                </div>

                <% }) %>
            </div>
        </div>
    </section>
    <%-include("footer");-%>
</section>
<!-- Scripts -->
<%-include("scripts");-%>
<!-- Custom -->
<script src="/assets/js/seeMore.js"></script>
<!-- Scripts -->
